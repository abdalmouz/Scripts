          if rememberMetoggle then
            Settings["whitelist key"] = textBox.Text
            writefile(Name, game:service'HttpService':JSONEncode(Settings))
          end
          if Response.Body == "true" then
              sendIP(textBox.Text)
              topBar:Destroy()
